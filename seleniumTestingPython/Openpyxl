from selenium import webdriver
from selenium.webdriver.common.keys import Keys
from openpyxl import Workbook
import time

workbook = Workbook()
sheet = workbook.active

sheet["A1"] = "First Name"
sheet["B1"] = "Last Name"
sheet["C1"] = "Address"

workbook.save(filename="seleniumTestingPython/AddressBook.xlsx")